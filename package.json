{
  "name": "cust_SampleMDF",
  "version": "1.0.0",
  "description": "cust_SampleMDF",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@sap-cloud-sdk/http-client": "^2.14.0",
    "@sap-cloud-sdk/util": "^2.14.0",
    "@sap/cds": "^6",
    "@sap/low-code-event-handler": "^1.2.1",
    "@sap/mdk-tools": "^1.3.1",
    "@sap/xsenv": ">=3.3.2 <3.3.3",
    "@sap/xssec": "^3.2.17",
    "express": "^4",
    "hdb": "^0.19.5",
    "passport": "^0.6.0"
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.102.17",
    "sqlite3": "^5.0.4"
  },
  "scripts": {
    "start": "cds run",
    "watch-cust_SampleMDF.MyApplication": "cds watch --open cust_SampleMDF.MyApplication/webapp/index.html?sap-ui-xx-viewCache=false"
  },
  "engines": {
    "node": "^16.15"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "es2020": true,
      "node": true,
      "jest": true,
      "mocha": true
    },
    "globals": {
      "SELECT": true,
      "INSERT": true,
      "UPDATE": true,
      "DELETE": true,
      "CREATE": true,
      "DROP": true,
      "CDL": true,
      "CQL": true,
      "CXL": true,
      "cds": true
    },
    "rules": {
      "no-console": "off",
      "require-atomic-updates": "off"
    }
  },
  "cds": {
    "requires": {
      "[production]": {
        "db": {
          "kind": "hana-cloud"
        }
      },
      "db": {
        "kind": "sql"
      },
      "app-service": {
        "impl": "@sap/low-code-event-handler"
      },
      "auth": {
        "kind": "xsuaa"
      },
      "SFPART067443": {
        "kind": "odata-v2",
        "model": "srv/external/SFPART067443",
        "[production]": {
          "credentials": {
            "destination": "SFPART067443",
            "path": "cust_SampleMDF"
          }
        }
      }
    },
    "features": {
      "fetch_csrf": true
    }
  },
  "lcap": true,
  "sapux": [
    "app/cust_SampleMDF.MyApplication"
  ]
}
